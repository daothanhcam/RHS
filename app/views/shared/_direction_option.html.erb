<div class="col-md-5 col-md-offset-3 shadow-z-2" id="direction-option-box" style="display:none">
	<div class="col-md-5">
		<label class="col-md-4" style="margin-top: 14px">Destination:</label>
		<div class="col-md-4" style="margin-top: 6px"><input class="form-control" id="des-lat" placeholder="Lat"></div>
		<div class="col-md-4" style="margin-top: 6px"><input class="form-control" id="des-lng" placeholder="Log"></div>
	</div>

	<div class="col-md-5">
		<b class="col-md-6" style="margin-top: 14px">Mode of Travel: </b>
		<div class="col-md-6" style="margin-top: 14px; padding-right: 0px;">
	    <select id="travelMode">
	      <option value="DRIVING">Driving</option>
	      <option value="WALKING">Walking</option>
	      <option value="BICYCLING">Bicycling</option>
	      <option value="TRANSIT">Transit</option>
	    </select>
		</div>
	</div>

	<div class="col-md-2" style="padding-left: 0px;">
		<button class="btn btn-success btn-sm" id="get-direction-submit" onclick="getDirection()">Get direction</button>
	</div>

	<div class="col-md-0">
		<div style="display: hidden;"><input class="form-control" id="start-lat"></div>
		<div style="display: hidden;"><input class="form-control" id="start-lng"></div>
	</div>
</div>
